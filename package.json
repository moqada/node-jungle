{
  "name": "node-jungle",
  "description": "AWS operations by cli should be simpler",
  "version": "0.4.1",
  "author": "moqada <moqada@gmail.com>",
  "bin": {
    "jungle": "bin/jungle"
  },
  "bugs": {
    "url": "https://github.com/moqada/node-jungle/issues"
  },
  "dependencies": {
    "aws-sdk": "^2.2.31",
    "aws-sdk-config-loader": "^1.0.0",
    "chalk": "^1.1.1",
    "cli-table": "^0.3.1",
    "commander": "^2.8.1",
    "debug": "^2.2.0",
    "minimatch": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.4",
    "babel-eslint": "^4.1.0",
    "babel-preset-es2015": "^6.1.4",
    "esdoc": "^0.4.3",
    "eslint": "^1.2.1",
    "eslint-config-moqada": "^8.0.1",
    "gh-pages": "^0.8.0",
    "npm-run-all": "^1.2.12"
  },
  "files": [
    "bin",
    "lib"
  ],
  "homepage": "https://github.com/moqada/node-jungle#readme",
  "keywords": [
    "aws",
    "cli",
    "shell"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/moqada/node-jungle.git"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "deploy": "npm-run-all doc gh-pages",
    "doc": "esdoc -c esdoc.json",
    "gh-pages": "gh-pages -d doc",
    "lint": "eslint src",
    "postpublish": "npm run deploy",
    "prebuild": "rm -rf lib",
    "prepublish": "git diff --exit-code && npm run --if-present build",
    "test": "npm-run-all lint build test:*",
    "test:example": "cd example && npm install && npm run jungle",
    "watch": "babel src --out-dir lib --watch"
  }
}
